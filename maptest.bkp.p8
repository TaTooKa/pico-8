pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
timer=0
tick=0 -- aprox 1 sec

gravity=2

tilew=8
tileh=8

level={}
level.offset={}
level.offset.x=18
level.offset.y=1
level.inipos={}
level.inipos.x=8
level.inipos.y=8

player={}
player.x=level.inipos.x
player.y=level.inipos.y
player.h=8
player.w=7
player.spd=1
player.flip=false
player.frames={52,53,54,55}
player.framei=1
player.spr=player.frames[1]
player.jumpspr=56
player.fallspr=57
player.animdelay=4
player.jumping=false
player.walking=false
player.jumpheight=16
player.cur_jumpheight=0
player.jumpspd=2

debuglines={"debug"}

function _update()
 timer+=1
 cls()

 countticks()
 
 moveplayer()
 
end

function _draw()
 drawmap()
 drawplayer()
 showdebug() --add to debug with debug("text",linepos)
end

function drawplayer()
 spr(player.spr,player.x,player.y,1,1,player.flip)
end

function moveplayer()
 if btn(0) then
  move("left")
  animateplayer()
 elseif btn(1) then
  move("right")
  animateplayer()
 else
  player.spr=player.frames[1]
  player.walking=false
 end
 
 if btnp(4) then
  startjump()
 end
 
 --gravity
 if not player.jumping then
  fall()
 else
  jump()
 end
end

function drawmap()
 initile=gettilepos(1,1,level)
 map(initile.x,initile.y,0,0,16,16)
end

function animateplayer()
 if not player.jumping then
  mframes={} 
  -- multiplied frames
  -- to make animation slower
  for frame in all(player.frames) do
   for i=1,player.animdelay do
    add(mframes,frame)
   end
  end
 
  player.spr=mframes[player.framei]
  if player.framei >= count(mframes) then
   player.framei=1
  else
   player.framei+=1
  end
  
 end
end

function move(dir)
 player.walking=true
 if dir=="left" then
  player.flip=true
 	dirmult=-1
 elseif dir=="right" then
  player.flip=false
  dirmult=1 
 end

 player.x+=player.spd*dirmult
 if iscolliding() then
  player.x-=player.spd*dirmult
 end
end

function countticks()
 if timer>5 then
  timer=0
  walkingsounds()
 end
end

function walkingsounds()
 if player.walking 
 and inground() then
  sfx(2)
 end
end

function iscolliding()
 pleftx=player.x+tilew-player.w-1
 prightx=pleftx+player.w-1
 ptopy=player.y
 pboty=player.y+player.h-1
 
 return solidc(pleftx,ptopy) or
 solidc(pleftx,pboty) or
 solidc(prightx,ptopy) or
 solidc(prightx,pboty)
 
end

function inground()
 player.y+=1
 colliding=iscolliding()
 player.y-=1
 return colliding
end
 
function touchingceiling()
 player.y-=1
 colliding=iscolliding()
 player.y+=1
 return colliding
end

function collideleft()
 p_left=player.x
 p_vmiddle=player.y+player.h/2
 
 tilepos=gettilepos(p_left,p_vmiddle,level)
 return solid(tilepos.x,tilepos.y)
end

function collideright()
 p_right=player.x+player.w
 p_vmiddle=player.y+player.h/2
 
 tilepos=gettilepos(p_right,p_vmiddle,level)
 return solid(tilepos.x,tilepos.y)
end

function startjump()
 if inground() then
  sfx(0)
  player.jumping=true
  player.cur_jumpheight=0
  --player.spr=56
  debug("jumping!",1)
 end
 
end

function jump()
 if player.cur_jumpheight<player.jumpheight then
  if not touchingceiling() then
   player.y-=player.jumpspd
   player.cur_jumpheight+=player.jumpspd
   player.spr=player.jumpspr
  else
   sfx(1)
   player.jumping=false
  end
 else
  player.jumping=false
  debug("not jumping",1)
 end 
end

function fall()

 if not inground() then
  debug("falling",2)
  player.y+=gravity
  player.spr=player.fallspr
  player.falling=true
 else
  debug("in ground",2)
  if player.falling then 
   sfx(3)
  end
  player.falling=false
 end
end

function gettilepos(x,y,lvl)
 tilepos={}
 offsetx=lvl.offset.x
 offsety=lvl.offset.y
 tilepos.x=flr(x/8)+offsetx
 tilepos.y=flr(y/8)+offsety
 return tilepos
end

function solid(x,y) --tile coords
 val=mget(x,y)
  -- 0 is the sprite flag 
  -- for a solid block
 return fget(val,0)
end

function solidc(x,y) --screen coords
 tilepos=gettilepos(x,y,level)
 val=mget(tilepos.x,tilepos.y)
  -- 0 is the sprite flag 
  -- for a solid block
 return fget(val,0)
end


--debug fuctions
function showdebug()
 posy=0
 for text in all(debuglines) do
  print(text,0,posy,7)
  posy+=6
 end
end

function debug(text,linepos)
 debuglines[linepos]=text
end

